<body>
<header>
<%- include ../../partials/navbar %>
</header>

<div class="container">
  <div class="col-md-6 mx-auto text-center">
	 <div class="header-title">
		<h1 class="wv-heading--title">
		Update Student Info
		</h1>
	 </div>
  </div>
  
  <hr size="6" width="100%" align="left" color="green">
  
  <div class="row">
	 <div class="col-md-4 mx-auto">
		<div class="myform form ">
		   
           
           <form action="/update1" method="post" >

			  <div class="form-group">
                <label for="example-text-input" class="col-2 col-form-label label">Name:</label>
				 <input type="text" name="s_name"  class="form-control my-input"  placeholder="Name"required>
			  </div>
			  
              <div class="form-group">
                <label for="example-text-input" class="col-2 col-form-label label">Phone:</label>
				 <input type="text"  name="s_phone"  class="form-control my-input" placeholder="Phone" >
			  </div>
			  <div class="form-group row">
                        <label for="example-text-input" class="col-2 col-form-label"></label>
                        <div class="col-10">
                            <input id="inputClass" class=" form-control  btn-outline-success " name="" type="submit" value="submit">
                        </div>
                    </div>
		   </form>
		</div>
	 </div>
  </div>
</div>

<div class="container">
  <div class="col-md-6 mx-auto text-center">
	 <div class="header-title">
		<h1 class="wv-heading--title">
		Update Branch Info
		</h1>
	 </div>
  </div>
  
  <hr size="6" width="100%" align="left" color="green">
  
  <div class="row">
	 <div class="col-md-4 mx-auto">
		<div class="myform form ">
		   
           
           <form action="/update2" method="post" >

			  <div class="form-group">
                <label for="example-text-input" class="col-2 col-form-label label">Name:</label>
				 <input type="text" name="b_name"  class="form-control my-input"  placeholder="Name"required>
			  </div>
			  
              <div class="form-group">
                <label for="example-text-input" class="col-2 col-form-label label">HOD:</label>
				 <input type="text"  name="b_hod"  class="form-control my-input" placeholder="HOD Name" >
			  </div>
			  <div class="form-group row">
                        <label for="example-text-input" class="col-2 col-form-label"></label>
                        <div class="col-10">
                            <input id="inputClass" class=" form-control  btn-outline-success " name="" type="submit" value="submit">
                        </div>
                    </div>
		   </form>
		</div>
	 </div>
  </div>
</div>



